class Solution {
    public void duplicateZeros(int[] arr) {
        for(int i=0;i<arr.length;i++){
            if(arr[i]==0){
                insert( arr,i++,0);
            }
        }
    }
   void insert(int [] arr,int n,int val){
        for(int i=arr.length-1;i>=n;i--){
            if(i!=0)
            arr[i]=arr[i-1];
            
        }
        arr[n]=val;
    }
}